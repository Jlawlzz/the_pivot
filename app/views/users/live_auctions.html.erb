
<div class="card z-depth-5" style="border-style:solid;border-color:#ff4444;display:inline-block;border-width:thin;" align="center">
  <%= link_to "<- Back to Dashboard", dashboard_path, style: "border-style:solid;border-color:#ff4444;width:225px;height:30px;padding-top:3px;display:inline-block;border-width:thin;" %>
</div>

<h1 class = "robot center"> Your current live auctions: </h1>
<table class="bordered striped highlight centered responsive-table">
  <thead class="column-head">
    <tr>
      <th data-field="Title">Auction Human</th>
      <th data-field="Time">Time Remaining</th>
      <th data-field="Your Last Bid">Your Last Bid</th>
      <th data-field="Current High Bid">Current High Bid</th>
    </th>
  </tr>
</thead>
<tbody>
  <%= render partial: "live_auctions", collection: @live_auctions, as: :auction %>
</tbody>
</table>
